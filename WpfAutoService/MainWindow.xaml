<Window x:Class="WpfAutoService.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAutoService"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Border BorderThickness="5" BorderBrush="#FF8C00">
        <Grid>
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="10*"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <StackPanel Grid.Row="0" Background="Gray" Height="auto" Width="auto">
                
            </StackPanel>
            <Frame  x:Name="frame" Grid.Row="1" Height="auto" Width="auto" LoadCompleted="frame_LoadCompleted">
                
                
            </Frame>
            <StackPanel Grid.Row="2" Background="Gray" Height="auto" Width="auto">
                
            </StackPanel>
        </Grid>
    </Border>
</Window>
